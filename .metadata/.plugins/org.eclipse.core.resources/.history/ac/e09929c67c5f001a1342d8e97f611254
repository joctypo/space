package model;

import processing.core.PApplet;

public class Logic extends PApplet{
	
	int posx;
	
	
	public static void main(String[] args) {
		PApplet.main("model.Logic");
	}
	
	public void setup() {
		
	
		posx=250;
		
	}

	public void draw() {
		mover();
	}

	public void anadirbalas() {
		fill(0);
		
	}

	public void mover() {
		
		posx=posx+1;
		
		
	}
	
	public int movio() {
		
		return posx;
	}





}
