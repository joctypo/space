package model;



public abstract class Personajes {
	
	private int posx;
	private int posy;
	private int Tamanno;
	
	public Personajes(int posX,int posY) {
		posx=posX;
		posy=posY;
		Tamanno=10;
		
	}

	public int getPosx() {
		return posx;
	}

	public void setPosx(int posx) {
		this.posx = posx;
	}

	public int getPosy() {
		return posy;
	}

	public void setPosy(int posy) {
		this.posy = posy;
	}

	public int getTamanno() {
		return Tamanno;
	}

	public void setTamanno(int tamanno) {
		Tamanno = tamanno;
	}
	
	
	
	
}
